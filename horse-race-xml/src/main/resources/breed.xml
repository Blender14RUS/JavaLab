<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="breed1" class="domain.Breed">
        <property name="name" value="Arabian"/>
        <property name="maxSpeed" value="70"/>
    </bean>

    <bean id="breed2" class="domain.Breed">
        <property name="name" value="Thoroughbred"/>
        <property name="maxSpeed" value="69"/>
    </bean>

    <bean id="breed3" class="domain.Breed">
        <property name="name" value="Warmblood"/>
        <property name="maxSpeed" value="68"/>
    </bean>

</beans>